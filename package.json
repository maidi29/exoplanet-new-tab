{
  "name": "exoplanet-newtab",
  "version": "0.1.0",
  "dependencies": {
    "@types/chrome": "0.0.135",
    "@types/node": "^12.20.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "caniuse-lite": "^1.0.30001741",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-html-parser": "^2.0.2",
    "react-scripts": "4.0.3",
    "react-tooltip": "^4.2.18",
    "rimraf": "^3.0.2",
    "sass": "^1.69.0",
    "typescript": "^4.2.4",
    "webpack-cli": "^4.6.0"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "test": "react-scripts test",
    "prebuild": "rimraf build",
    "build": "npm-run-all build:*",
    "build:app": "set \"INLINE_RUNTIME_CHUNK=false\" && set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "build:bg": "set \"NODE_ENV=production\" && set \"BABEL_ENV=production\" && set \"NODE_OPTIONS=--openssl-legacy-provider\" && webpack --config webpack.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "env": {
      "browser": true,
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-html-parser": "^2.0.1"
  }
}
